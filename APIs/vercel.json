{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "headers": [
    {
      "key": "Content-Security-Policy",
      "value": "script-src 'self' https://www.gstatic.com 'unsafe-inline' 'unsafe-eval' blob: data;"
    },
    {
      "key": "Access-Control-Allow-Origin",
      "value": "*"
    },
    {
      "key": "Access-Control-Allow-Credentials",
      "value": "true"
    }
  ]
}