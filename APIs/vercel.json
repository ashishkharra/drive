{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "name": "Content-Security-Policy",
          "value": "script-src 'self' https://www.gstatic.com 'unsafe-inline' 'unsafe-eval' blob: data:;"
        },
        {
          "name": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "name": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ]
}